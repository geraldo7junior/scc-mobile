<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background1"
    tools:context=".Home" >

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/imageView1"
        android:layout_marginRight="14dp"
        android:src="@drawable/consultoresico" android:contentDescription="@string/consultoresico"/>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="27dp"
        android:src="@drawable/agendaico"
        android:contentDescription="@string/agendaico" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView5"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_marginBottom="18dp"
        android:src="@drawable/empresasico"
        android:contentDescription="@string/empresasico" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView3"
        android:layout_marginBottom="22dp"
        android:layout_marginRight="16dp"
        android:layout_toLeftOf="@+id/imageView2"
        android:src="@drawable/projetosico"
        android:contentDescription="@string/projetosico" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView2"
        android:layout_alignRight="@+id/imageView5"
        android:layout_marginBottom="24dp"
        android:src="@drawable/logo"
        android:contentDescription="@string/logo" />

    <Button
        android:id="@+id/btnapontamentos"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_alignRight="@+id/imageView2"
        android:layout_below="@+id/imageView1"
        android:background="@drawable/apontamentosico" />

</RelativeLayout>